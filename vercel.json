{
  "crons": [
    {
      "path": "/api/reconcile-permissions?token=${RECONCILIATION_SECRET_TOKEN}",
      "schedule": "0 3 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/reconcile-permissions",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache"
        }
      ]
    }
  ]
}
