{
  "crons": [
    {
      "path": "/api/reconcile-permissions",
      "schedule": "0 3 * * *",
      "headers": {
        "x-cron-auth": "${RECONCILIATION_SECRET_TOKEN}"
      }
    }
  ],
  "headers": [
    {
      "source": "/api/reconcile-permissions",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache"
        }
      ]
    }
  ]
}
